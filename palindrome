import java.util.*;
class Palindrome{
public static void main(String args[])
{
int n;
int rev=0;int rem;
Scanner sc=new Scanner(System.in);
n=sc.nextInt();
int num=n;
while(n>0){
rem=n%10;
rev=rev*10+rem;
n=n/10; 
sc.close();
}
if(rev==num)
{
System.out.print("Palindrome");
} 
else{
System.out.print("Non Palindrome");
}
}
}